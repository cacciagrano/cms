        "default": "moderate_progress",
        "description": "Evaluación del progreso realizado"
      },
      "areasForImprovement": {
        "type": "text",
        "description": "Áreas identificadas para mejorar"
      },
      "submittedAt": {
        "type": "datetime",
        "description": "Fecha y hora de envío del feedback"
      }
